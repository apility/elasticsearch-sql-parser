{
    "name": "apility/elasticsearch-sql-parser",
    "description": "Library for parsing SQL into the Elasticsearch DSL",
    "type": "library",
    "keywords": [
        "sql",
        "elasticsearch"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Alrek",
            "email": "thomas@apility.no"
        }
    ],
    "require": {
        "php": ">=7.4",
        "greenlion/php-sql-parser": "^4.5",
        "guzzlehttp/guzzle": "7.4.x-dev"
    },
    "autoload": {
        "psr-4": {
            "Apility\\ElasticSearch\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpstan/phpstan": "^1.8"
    },
    "scripts": {
        "analyze": "phpstan analyse -c phpstan.neon"
    }
}